<% let latestPlan = plans.length - 1 %>
<% console.log(plans[latestPlan]) %>
<div id="pageStructure" class="px-[120px] py-[40px] w-screen h-fit grid grid-col-5 gap-16">
    <div id="planSection" class="bg-pink-200 min-h-[400px] col-start-1 col-end-1">
        <div class="bg-red-100 h-[300px] min-w-[350px]" >
            <a href="/plans">Create Plan</a>
        </div>
    </div>
    <div id="showPlan" class=" bg-blue-200 min-h-[700px] w-full col-start-2 col-end-5">
        <div class=" p-10 h-1/6 bg-pink-800 w-full uppercase text-[12px] box-border">
            <img src="<%= plans[latestPlan].banner_image %>" alt="banner" class="max-h-[200px] max-w-[500px]">
        </div>
        <div class=" p-10 h-1/6 bg-pink-300 w-full uppercase text-[32px]">title: <%= plans[latestPlan].name %></div>
        <div class=" p-10 h-1/6 bg-pink-400 w-full uppercase text-[22px]">description: <%= plans[latestPlan].description %></div>
        <div class=" p-10 h-3/6 bg-pink-500 content-wrap uppercase text-[24px]">content: <%= plans[latestPlan].content %></div>
        

    </div>

</div>